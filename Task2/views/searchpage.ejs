<div class="heading">
    <h1>Search Results</h1>
</div>


<section  class="articles">

    <ul class="article-ul">

        <% data.forEach(post => { %>    <!--for loop-->

        <li>
            <a href="/post/<%= post._id %>">
                <span>
                    <%= post.title %>
                </span>
                <span class="art-date">
                    <%= post.CreatedAt.toDateString() %>
                </span>
            </a>
        </li>

        <% }) %>
       
    </ul>    

</section>